{
  "name": "demo-card",
  "description": "A card element that can display an avatar or initials on one side and employee information on the other.",
  "version": "1.0.0",
  "jetVersion": ">=3.0.0",
  "properties": {
    "name": {
      "description": "The employee's full name.",
      "type": "string"
    },
    "avatar": {
      "description": "The url of the employee's image.",
      "type": "string"
    },
    "workTitle": {
      "description": "The employee's job title.",
      "type": "string"
    },
    "workNumber": {
      "description": "The employee's work number.",
      "type": "number"
    },
    "email": {
      "description": "The employee's email.",
      "type": "string"
    }
  },
  
  "methods" : {
      "changeName" : {
          "params" : [ {
              "name" : "val",
              "type" : "string",
              "description" : "The value to be updated"}],
          "return" : "boolean"
      }
  }
}